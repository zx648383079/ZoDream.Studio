<Window x:Class="ZoDream.Studio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZoDream.Studio"
        mc:Ignorable="d"
        Title="ZoDream Studio" Height="450" Width="800" 
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded"
        Unloaded="Window_Unloaded">

    <Grid Background="{DynamicResource BodyBrush}">
        <Frame x:Name="InnerFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
